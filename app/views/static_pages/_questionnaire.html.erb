<div class="row-fluid">
	<div class="span12">
		<div class="btn-group worried-list" data-toggle="buttons-checkbox">
			<% @worried_list.each do |item| %>
				<%= button_to item, survey_results_path(survey_result:{content:item}), remote: true, class: "btn btn-info survey2 worried-item"%>
			<% end %>
		</div>
		<div class="btn-group excited-list" data-toggle="buttons-checkbox">
			<% @excited_list.each do |item| %>
				<%= button_to item, survey_results_path(survey_result:{content:item}), remote: true, class: "btn btn-info survey2 excited-item"%>
			<% end %>
		</div>
	</div>
</div>
<div class="btn-group survey1-group" data-toggle="buttons-radio">
<div class="row-fluid">
	<div class="span6">
		<button type="button" class="btn btn-primary btn-large survey1 worried-button">Worried?</button>
	</div>
	<div class="span6">
		<button type="button" class="btn btn-primary btn-large survey1 excited-button">Excited!</button>
	</div>
</div>
